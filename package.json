{
  "name": "freeze-mutate",
  "version": "1.1.3",
  "description": "Utility library for immutable objects management",
  "keywords": [
    "immutable",
    "mutate",
    "freeze",
    "pojo",
    "DTO",
    "javascript",
    "typescript"
  ],
  "author": "eram@weblegions.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eram/freeze-mutate/issues"
  },
  "homepage": "https://github.com/eram/freeze-mutate#readme",
  "main": "./build/dist/index.js",
  "typings": "./build/dist/index.d.ts",
  "scripts": {
    "build": "npm run build:debug",
    "build:debug": "tsc -p ./tsconfig.debug.json --pretty",
    "build:prod": "tsc -p ./tsconfig.release.json --pretty",
    "test": "jest --coverage --no-cache --config ./jest.json",
    "test:nocover": "jest --config ./jest.json",
    "test:watch": "jest --watch --config ./jest.json",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node --project ./tsconfig.debug.json --pretty -- ./src/index.ts",
    "clean": "rimraf ./build",
    "pretest": "npm run clean && npm run lint && npm run build",
    "lint": "eslint --ext .ts ./src",
    "xlint": "tslint --format stylish --config ./config/tslint.json --project ./config/tsconfig.json",
    "fix": "eslint --ext .ts --fix ./src"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
